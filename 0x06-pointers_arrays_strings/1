#include "main.h"

/**
 * _strcat -concatnates the two strins needed
 * @dest:destination string value
 * @src:src string value
 * Return: concatnated string.
 */
char *_strcat(char *dest, char *src)
{
	int i = 0, j;

	while (dest[i])
	{
		i++;
	}

	for (j = 0; src[j] != '\0'; j++)
	{
		dest[i] = src[j];
		i++;
	}
	dest[i] = '\0';

	return (dest);

}
